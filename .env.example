# Python 환경 변수 설정 예시
# 이 파일을 복사하여 .env 파일을 생성하고 실제 값을 입력하세요

# 애플리케이션 설정
ENVIRONMENT=development
DEBUG=True
SECRET_KEY=your_secret_key_here

# 데이터베이스 설정
DATABASE_URL=postgresql://user:password@localhost:5432/dbname
DATABASE_HOST=localhost
DATABASE_PORT=5432

# API 설정
API_KEY=your_api_key_here

# 기타 설정
LOG_LEVEL=INFO
